    "@chakra-ui/react": "2.5.5",
    "@duckdb/duckdb-wasm": "^1.29.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.4",
    "@ladle/react": "^5.0.1",
    "@tanstack/react-query": "4",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "apache-arrow": "^17.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "framer-motion": "10.18.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
