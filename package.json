{
  "name": "sqlrooms",
  "version": "1.0.0",
  "repository": "git@github.com:ilyabo/sqlrooms.git",
  "author": "Ilya Boyandin <ilya@boyandin.me>",
  "private": true,
  "packageManager": "pnpm@9.15.1",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "turbo dev --concurrency=12 & pnpm run ladle",
    "clean": "rimraf \"packages/*/dist\"",
    "build": "pnpm clean && turbo build",
    "test": "turbo test",
    "prepare": "husky",
    "lint": "eslint --quiet --fix \"**/*.{ts,tsx}\"",
    "typescript": "tsc --noEmit",
    "ladle": "ladle serve",
    "graph": "turbo run build --graph"
  },
  "devDependencies": {
    "@chakra-ui/icons": "2.1.0",
    "@chakra-ui/react": "2.4.4",
    "@duckdb/duckdb-wasm": "^1.29.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.4",
    "@eslint/js": "^9.18.0",
    "@ladle/react": "^5.0.1",
    "@tanstack/react-query": "4",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@typescript-eslint/parser": "^8.19.1",
    "apache-arrow": "^18.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "framer-motion": "10.18.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^5.0.5",
    "turbo": "2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "jest": "^29.7.0"
  },
  "pnpm": {
    "overrides": {
      "apache-arrow": "18.1.0",
      "@chakra-ui/react": "2.4.4"
    }
  },
  "dependencies": {
    "remove": "^0.1.5"
  }
}
